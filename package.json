    "description": "Datepicker with input and calendar",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "dev": "craco start --no-cache",
        "test": "jest --maxWorkers=2 --config ./jest/jest.config.js",
